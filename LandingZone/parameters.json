{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "naming": {
            "value": {
                "prefix": "uks",
                "prefix1": "prod"
            }
        },
        "resources": {
            "value": {
                "resourcegroups": [
                    "hub-rg",
                    "diag-rg",
                    "gateway-rg"
                ],
                "rg":"lz-rg-diag",
                "location": "uksouth",
                "tags": {
                    "general": "work",
                    "effort:": "nothing"
                }
            }
        },
        "diagnostics": {
            "value": {
                "retentionpolicy": {
                    "days": 7,
                    "enabled": true
                },
                "loganalytics": {
                    "properties": {
                        "features": {
                            "enableDataExport": true,
                            "enableLogAccessUsingOnlyResourcePermissions": true,
                            "immediatePurgeDataOn30Days": false
                        },
                        "forceCmkForQuery": false,
                        "retentionInDays": 60,
                        "sku": {
                            "name": "PerGB2018"
                        },
                        "workspaceCapping": {
                            "dailyQuotaGb": 10
                        }
                    }
                },
                "storageaccount": {
                    "globalredundancy": true
                }
            }
        },
        "networking": {
            "value": {
                "virtualnetwork": {
                    "virtualnetworkname": "vnet1",
                    "virtualnetworktags": {
                        "costcentre": "tag",
                        "place": "nottingham"
                    },
                    "properties": {
                        "addressSpace": {
                            "addressPrefixes": [
                                "172.10.0.0/16"
                            ]
                        },
                        "enableVmProtection": true,
                        "enableDdosProtection": false,
                        "subnets": [
                            {
                                "name": "subnet1",
                                "properties": {
                                    "addressprefix": "172.10.20.0/24",
                                    "privateEndpointNetworkPolicies": "Disabled"
                                }
                            },
                            {
                                "name": "GatewaySubnet",
                                "properties": {
                                    "addressprefix": "172.10.21.0/24"
                                }
                            },
                            {
                                "name": "AzureFirewallSubnet",
                                "properties": {
                                    "addressprefix": "172.10.22.0/24"
                                }
                            }
                        ]
                    }
                },
                "privateendpoints": {
                    "privatednszonenames": [
                        "privatelink.blob.core.windows.net",
                        "privatelink.file.core.windows.net"
                    ]
                },
                "gateway": {
                    "activeactive": false,
                    "enablebgp": false,
                    "enablednsforwarding": false,
                    "enableprivateipaddress": false,
                    "gatewayipconfigurationname": "ipconfigur01",
                    "gatewaytype": "VPN",
                    "vpntype": "RouteBased",
                    "gatewaysku": {
                        "name": "VpnGw1AZ",
                        "tier": "VpnGw1AZ"
                    },
                    "gatewaygeneration": "Generation1",
                    "privateipallocation": "Dynamic"
                },
                "gatewaypublicip": {
                    "sku": {
                        "name": "standard"
                    },
                    "properties": {
                        "publicIPAddressVersion": "IPv4",
                        "publicIPAllocationMethod": "Static",
                        "idleTimeoutInMinutes": 4
                    },
                    "zones": [
                        "1"
                    ]
                },
                "localgateway": {
                    "properties": {
                        "localNetworkAddressSpace": {
                            "addressPrefixes": [
                                "10.156.64.0/19"
                            ]
                        },
                        "gatewayIpAddress": "109.232.61.236"
                    }
                },
                "firewall": {
                    "azfwpublicip": {
                        "sku": {
                            "name": "standard"
                        },
                        "properties": {
                            "publicIPAddressVersion": "IPv4",
                            "publicIPAllocationMethod": "Static",
                            "idleTimeoutInMinutes": 4
                        },
                        "zones": [
                            "1",
                            "2"
                        ]
                    },
                    "properties": {
                        "sku": {
                            "name": "AZFW_VNet",
                            "tier": "Standard"
                        },
                        "zones": [
                            "1",
                            "2"
                        ],
